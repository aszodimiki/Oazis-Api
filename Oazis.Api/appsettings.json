{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "D:/OwnWorks/Oazis/Oazis.Api/Logs/log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "2af1b455-8e6e-49f1-9375-1c851a9fd63c",
        "SanitizeTinyMce": true
      },
      "WebRouting": {
        "UmbracoApplicationUrl": "https://localhost:44370/"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "ModelsBuilder": {
        "ModelsDirectory": "D:/Works/Oazis-Api/Oazis.Domain/ModelsBuilder",
        "ModelsNamespace": "Oazis.Domain.ModelsBuilder",
        "ModelsMode": "SourceCodeManual",
        "AcceptUnsafeModelsDirectory": true
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false,
        "BackOfficeHost": "https://localhost:44370/"
      }
    }
  },
  "uSync": {
    "Settings": {
      "RootFolder": "uSync/v14/",
      "DefaultSet": "Default",
      "ImportAtStartup": "None",
      "ExportAtStartup": "None",
      "ExportOnSave": "All",
      "UiEnabledGroups": "",
      "ReportDebug": false,
      "AddOnPing": true,
      "RebuildCacheOnCompletion": false,
      "FailOnMissingParent": false,
      "CacheFolderKeys": true,
      "ShowVersionCheckWarning": true,
      "CustomMappings": {},
      "SignalRRoot": "",
      "EnableHistory": true,
      "DefaultExtension": "config",
      "MyCustomSetting": true
    },
    "Sets": {
      "Default": {
        "Enabled": true,
        "HandlerGroups": [],
        "DisabledHandlers": [
          "ContentHandler-CO",
          "ContentTemplateHandler-CO",
          "ContentTypeHandler-CO",
          "DataTypeHandler-CO",
          "DictionaryHandler-CO",
          "DomainHandler-CO",
          "LanguageHandler-CO",
          "MacroHandler-CO",
          "MediaHandler-CO",
          "MediaTypeHandler-CO",
          "MemberTypeHandler-CO",
          "RelationTypeHandler-CO",
          "TemplateHandler-CO"
        ],
        "HandlerDefaults": {
          "Enabled": true,
          "Actions": [],
          "UseFlatStructure": true,
          "GuidNames": false,
          "FailOnMissingParent": false,
          "Group": "",
          "Settings": {}
        },
        "Handlers": {
          "ContentHandler": {
            "Settings": {
              "CreateOnly": true
            }
          }
        }
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=D:/Works/SQLiteDatabases/OazisDB.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  }
}